<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:WpfTutorials">
    <Color x:Key="BackGround">#efefef</Color>
    <SolidColorBrush x:Key="BackGroundBrush" Color="{StaticResource BackGround}"/>

    <Color x:Key="BackGroundLight">#eeffee</Color>
    <SolidColorBrush x:Key="BackGroundBrushLight" Color="{StaticResource BackGroundLight}"/>

    <Color x:Key="ForegroundMain">#686868</Color>
    <SolidColorBrush x:Key="ForegroundMainbrush" Color="{StaticResource ForegroundMain}"/>

    <Color x:Key="ForegroundDark">#000000</Color>
    <SolidColorBrush x:Key="ForegroundDarkBrush" Color="{StaticResource ForegroundDark}"/>

    <Color x:Key="ForeGroundLight">#ffffff</Color>
    <SolidColorBrush x:Key="ForegroundLightBrush" Color="{StaticResource ForeGroundLight}"/>
</ResourceDictionary>